                init_fnc = "/*Begin code section*/
// Initialize variables
p = +100;
q = +100;
mod_vrms_2 = 48400;
phi_vrms_2 = 0;
mod_s = 141.421356237;
phi_s = 0;
r0 = 243;
x0 = 1;
l0 = 0.65;
c0 = 1.370135529372377e-03;
is_inductive = 0;
/*End code section*/"
                input_terminals = "real IN_P0;real IN_Q0;"
                input_terminals_dimensions = "1;1"
                input_terminals_feedthrough = "True;True"
                input_terminals_show_labels = "False;False"
                output_fnc = "/*Begin code section*/
// Active power ramp
if(IN_P0 > p) p += 1;
if(IN_P0 < p) p -= 1;
    
// Reactive power ramp
if(IN_Q0 > q) q += 1;
if(IN_Q0 < q) q -= 1;

// base V_{RMS} in polar coordinates
mod_vrms_2 = 220*220; // V 
phi_vrms_2 = 0; // rad
// base S in polar coordinates
mod_s = sqrt(p*p + q*q); // VA
if(IN_Q0 >= 0 && IN_P0 >= 0 && mod_s < 141)
{
    mod_s = 141.421356237;
    p = +100;
    q = +100;
}
if(IN_Q0 >= 0 && IN_P0 <= 0 && mod_s < 141)
{
    mod_s = 141.421356237;
    p = -100;
    q = +100;
}
if(IN_Q0 <= 0 && IN_P0 >= 0 && mod_s < 141)
{
    mod_s = 141.421356237;
    p = +100;
    q = -100;
}
if(IN_Q0 <= 0 && IN_P0 <= 0 && mod_s < 141)
{
    mod_s = 141.421356237;
    p = -100;
    q = -100;
}
phi_s = atanf(q/p); // rad
// compute equivalent impedance Z = V^2/S = Z = R0 + j * X0
r0 = (mod_vrms_2/mod_s) * cos(phi_vrms_2 - phi_s);
x0 = (mod_vrms_2/mod_s) * sin(phi_vrms_2 - phi_s);
// verify if load is capactive or inductive
if(q  >= 0)
{
    is_inductive = 1;
    l0 =  fabs(x0) / 377;
    c0 = 0;
}else
{
    is_inductive = 0;
    l0 = 0;
    c0 = 1.0 / (377 * fabs(x0));
}

// Handle outputs
OUT_R0 = r0; //2.8;
OUT_L0 = l0; //1.3e-3;
OUT_C0 = c0; //1.3e-3;
OUT_IS_INDUCTIVE = is_inductive;
OUT[0] = r0;
OUT[1] = l0;
OUT[2] = c0;
/*End code section*/"
