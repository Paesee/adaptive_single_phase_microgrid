if(IN_ENABLE == 1){
    if(IN_MICROGRID_STATUS == 1){
        // Set status to tied
        setStatus(&droop, TIED);
        
        // Active power ramp
        if(IN_P0 > p_ref) p_ref += 5;
        if(IN_P0 < p_ref) p_ref -= 5;
            
        // Reactive power ramp
        if(IN_Q0 > q_ref) q_ref += 5;
        if(IN_Q0 < q_ref) q_ref -= 5;
            
        // Set new power reference
        setP0(&droop, p_ref);
        setQ0(&droop, q_ref);
        
